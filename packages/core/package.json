    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.15.18",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "rollup": "^3.29.5",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",

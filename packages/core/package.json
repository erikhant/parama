{
  "name": "@parama-dev/form-builder-core",
  "version": "0.1.1",
  "description": "Core functionality for the form builder system",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",

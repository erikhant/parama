{
  "name": "@parama-dev/form-builder-renderer",
  "version": "0.6.0",
  "description": "React components for rendering forms from the form builder",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "clean": "rimraf dist node_modules"
  },
  "dependencies": {
    "@parama-dev/form-builder-core": "^0.5.0",
    "@parama-dev/form-builder-types": "^0.3.3",
    "@parama-ui/react": "^1.3.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.510.0",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.18",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.29.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3"
  },
  "peerDependencies": {
    "@parama-dev/form-builder-core": "^0.5.0",
    "@parama-dev/form-builder-types": "^0.3.3",
    "@parama-ui/react": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  }
}
